<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.alpha.commons.core.mapper.StreetMapper">
    <select id="getStreetByName" resultType="java.lang.String" >
       SELECT `name` from sys_street WHERE  name LIKE '%${address}%' limit 100
    </select>
</mapper>